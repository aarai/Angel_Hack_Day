%h1 Presciption Program Signup

-# Down to line 25 is commented out
  %table
    %tr
      %th Name
      %th Dob
      %th Address
      %th Email
      %th Phone
      %th
      %th
      %th

    - @users.each do |user|
      %tr
        %td= user.name
        %td= user.dob
        %td= user.address
        %td= user.email
        %td= user.phone
        %td= link_to 'Show', user
        %td= link_to 'Edit', edit_user_path(user)
        %td= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete 

%br

.btn.btn-success
  = link_to 'Program Signup', new_user_path

- @users.each do |user|
  -# .btn.btn-warning
  -#   = link_to 'Show', user
  .btn.btn-warning
    = link_to 'Edit Signup Info', edit_user_path(user)
  .btn
    = link_to 'View Scripts', prescriptions_path
  .btn.btn-primary
    = link_to 'Fill New Script', new_prescription_path
  .btn.btn-danger
    = link_to 'Close Account', user, :confirm => 'Are you sure you want to close your account?', :method => :delete